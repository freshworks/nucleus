{
  "name": "@freshworks/nucleus",
  "version": "0.1.0",
  "private": true,
  "description": "Freshworks Design System in Ember",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "yarn workspace nucleus start",
    "test": "lerna run test --parallel --since origin/master",
    "build": "lerna run build --parallel --no-private",
    "lint": "lerna run lint:hbs",
    "publish": "lerna publish --yes",
    "deploy": "yarn workspace nucleus deploy",
    "node-test": "lerna run node-test",
    "try": "ember try:each",
    "try:legacy": "ember try:one ember-lts-2.18"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.0",
    "ember-cli": "~3.11.0",
    "ember-cli-babel": "^7.1.3",
    "ember-source-channel-url": "^2.0.0",
    "ember-try": "^1.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-ember": "^7.0.0",
    "eslint-plugin-node": "^10.0.0",
    "lerna": "^3.0.0",
    "sass": "^1.23.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  }
}